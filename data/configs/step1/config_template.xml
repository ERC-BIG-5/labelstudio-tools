        <Header value="Stewardship type" size="4"/>

        <View className="V_visual" visibleWhen="choice-selected" whenTagName="nature_visual"
              whenChoiceValue="Yes">

            <Text value="What are the Stewardship types in the VISUAL? (default: None)"
                  name="inter_group-1_visual_t"/>
            <Choices name="inter_group-1_visual" toName="title" showInline="true" choice="multiple">
                <Choice value="Preservation" alias="preservation"/>
                <Choice value="Restoration" alias="restoration"/>
                <Choice value="Sustainable use of resources" alias="sustainable_use_of_resources"/>
                <Choice value="Advocacy" alias="advocacy"/>
                <Choice value="Education" alias="education"/>
                <Choice value="Monitoring" alias="monitoring"/>
                <Choice value="Informal enforcement" alias="informal_enforcement"/>
                <Choice value="Other" alias="other"/>
