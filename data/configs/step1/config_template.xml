                <Choice value="Social responsibility" alias="social-responsibility"/>
                <Choice value="Social cohesion" alias="social-cohesion"/>
                <Choice value="Social memory" alias="social-memory"/>
                <Choice value="Social relations" alias="social-relations"/>
                <Choice value="Sense of place" alias="sense-of-place"/>
                <Choice value="Sense of agency" alias="sense-of-agency"/>
                <Choice value="Spirituality" alias="spirituality"/>
                <Choice value="Stewardship principle" alias="stewardship-principle"/>
                <Choice value="Stewardship eudaimonia" alias="stewardship-eudaimonia"/>
                <Choice value="Literacy" alias="literacy"/>
                <Choice value="Livelihoods" alias="livelihoods"/>
                <Choice value="Well-being" alias="well-being"/>
                <Choice value="Aesthetics" alias="aesthetics"/>
                <Choice value="Reciprocity" alias="reciprocity"/>
                <Choice value="Good life" alias="good-life"/>
                <Choice value="Kinship" alias="kinship"/>
            </Choices>

            <Text name="rel-value_visual_expl_t" value="Optional comments"/>
            <TextArea name="rel-value_visual_expl" toName="title" showSubmitButton="true"/>

            <!-- CONFUSION VISUAL-->
            <View>
                {{{VISUAL_RV_CONFUSION}}}
            </View>

        </View>

        <!-- Nature Elements/Processes -->
        <Header value="Nature Elements/Processes"/>
        <View className="V_visual" visibleWhen="choice-selected" whenTagName="nature_visual"
              whenChoiceValue="Yes">

            <Text value="What are the Nature Elements/Process in the VISUAL?"
                  name="nep_visual_t"/>
            <Text value="Materiality" name="nep_group-0_visual_t"/>
            <Choices name="nep_group-0_visual" toName="title" showInline="true" required="true" choice="multiple"
                     requiredMessage="Select one option from the Nature Element/Process: Materiality in VISUAL">
                <Choice value="Material" alias="material"/>
                <Choice value="Immaterial" alias="immaterial"/>
            </Choices>
            <Text name="nep_group-0_visual_expl_t" value="Optional comments"/>
            <TextArea name="nep_group-0_visual_expl" toName="title"/>

            <Text value="Biological" name="nep_group-1_visual_t"/>
            <Choices name="nep_group-1_visual" toName="title" showInline="true" required="true" choice="multiple"
                     requiredMessage="Select one option from the Nature Element/Process: Biological in VISUAL">
                <Choice value="Biotic" alias="biotic"/>
                <Choice value="Abiotic" alias="abiotic"/>
            </Choices>
            <Text name="nep_group-1_visual_expl_t" value="Optional comments"/>
            <TextArea name="nep_group-1_visual_expl" toName="title"/>

            <Text value="Landscape type (default: None)" name="nep_group-2_visual_t"/>
            <Choices name="nep_group-2_visual" toName="title" showInline="true" required="false" choice="multiple">
                <Choice value="Artificial surfaces" alias="artificial_surfaces"/>
                <Choice value="Agricultural areas" alias="agricultural_areas"/>
                <Choice value="Forest and seminatural areas" alias="forest_and_seminatural_areas"/>
                <Choice value="Wetlands" alias="wetlands"/>
                <Choice value="Water bodies" alias="water_bodies"/>
                <Choice value="Other" alias="other"/>
            </Choices>
            <View visibleWhen="choice-selected" whenTagName="nep_group-2_visual" whenChoiceValue="other">
                <Text name="nep_group-2_visual_other_t" value="Describe the Landscape type"/>
                <TextArea name="nep_group-2_visual_other" toName="title"/>
            </View>

            <Text name="nep_group-2_visual_expl_t" value="Optional comments"/>
            <TextArea name="nep_group-2_visual_expl" toName="title"/>
        </View>

        <!-- Interactions -->
        <Header value="Types of Interactions"/>
        <Header value="Basic type" size="4"/>

        <!-- Interaction VISUAL -->
        <View className="V_visual" visibleWhen="choice-selected" whenTagName="nature_visual"
              whenChoiceValue="Yes">
            <Text value="What are types of Interactions in the VISUAL?"
                  name="inter_group-0_visual_t"/>
            <Choices name="inter_group-0_visual" toName="title" showInline="true" required="true"
                     requiredMessage="Select one option from the Interaction type in VISUAL" choice="multiple">
                <Choice value="Human-human in/about nature" alias="human-human_in/about_nature"/>
                <Choice value="Human-nature" alias="human-nature"/>
                <Choice value="Nature-nature" alias="nature-nature"/>
                <Choice value="Nature" alias="nature"/>
            </Choices>
            <Text name="inter_group-0_visual_expl_t" value="Optional comments"/>
            <TextArea name="inter_group-0_visual_expl" toName="title"/>
