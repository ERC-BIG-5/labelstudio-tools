        <!-- // Sentiment -->

        <!-- Framing -->
        <Header value="Framing"/>

        <View className="V_visual" visibleWhen="choice-selected" whenTagName="nature_visual"
              whenChoiceValue="Yes">
            <Text value="VISUAL Framing" name="framing_visual_t"/>
            <Choices name="framing_visual" toName="title" showInline="true" choice="multiple">
                <Choice value="Informative" alias="informative"/>
                <Choice value="Emotive" alias="emotive"/>
                <Choice value="Reflective" alias="reflective"/>
