    <Header value="Do you want to mark this task for the 'coding game'? (Default: No)"/>
    <Choices name="for_coding_game" toName="title" showInline="true">
        <Choice value="Yes"/>
    </Choices>

    <View>
        <Header value="User categorization"/>

        <Header value="General" size="6"/>
        <Choices name="user-type_0" toName="title" showInline="true">
            <Choice value="Individual user" alias="individual_user"/>
            <Choice value="Influencer" alias="influencer"/>
            <Choice value="Media outlet" alias="media_outlet"/>
            <Choice value="Company" alias="company"/>
            <Choice value="NGO" alias="ngo"/>
            <Choice value="Educational institution" alias="educational_institution"/>
            <Choice value="Governmental institution" alias="governmental_institution"/>
