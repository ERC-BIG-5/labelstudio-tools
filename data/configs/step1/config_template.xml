        </View>

        <Header value="Gender" size="6"/>
        <Choices name="user-type_1" toName="title" showInline="true">
            <Choice value="Man-performing" alias="man-performing"/>
            <Choice value="Woman-performing" alias="woman-performing"/>
            <Choice value="Non-binary performing" alias="non-binary_performing"/>
