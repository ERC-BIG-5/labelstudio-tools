<Panel value="{{{title}}}">
    <slot/>
    <nature title="{{{title}}}" var="{{{var}}}"/>
    <View if="nature_{{{var}}}" is="Yes">
        <nature_elements var="{{{var}}}" title="{{{title}}}"/>
        <value-expr title="Frame {{{index}}}" suffix="{{{var}}}"/>
        <relational-value title="Frame {{{index}}}" var="{{{var}}}"/>
    </View>
</Panel>